  <maintainer email="alexander.tiderko@fkie.fraunhofer.de">Alexander Tiderko</maintainer>
  <license>GPLv2</license>
  <url type="website">https://github.com/fkie/iop_core.git</url>


  <buildtool_depend>catkin</buildtool_depend>
  <build_depend>iop_accesscontrol_fkie</build_depend>
  <build_depend>iop_discovery_fkie</build_depend>
  <build_depend>iop_management_fkie</build_depend>
  <build_depend>iop_msgs_fkie</build_depend>
  <build_depend>iop_ocu_controllib_fkie</build_depend>
  <build_depend>roscpp</build_depend>
  <run_depend>roscpp</run_depend>
  <run_depend>iop_accesscontrol_fkie</run_depend>
  <run_depend>iop_discovery_fkie</run_depend>
